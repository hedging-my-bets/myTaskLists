
{
  "expo": {
    "name": "PetProgress",
    "slug": "PetProgress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#60A5FA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petprogress.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.petprogress.app"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#60A5FA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.petprogress.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "petprogress"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "@bacons/apple-targets",
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "petprogress",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
